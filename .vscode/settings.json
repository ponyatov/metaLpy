{
"python.pythonPath"              : "./bin/python3",
"python.formatting.provider"     : "autopep8",
"python.formatting.autopep8Path" : "./bin/autopep8",
"python.formatting.autopep8Args" : ["--ignore=E26,E302,E401,E402"],
/* / <section:top> */
/* \ <section:mid> */
/* \ <section:multi> */
"multiCommand.commands": [
    {
        "command": "multiCommand.f11",
        "sequence": [
            "workbench.action.files.saveAll",
            {"command": "workbench.action.terminal.sendSequence","args": {"text":
                "\u000D clear ; make pep test doxy \u000D"
            }}]
    },
    {
        "command": "multiCommand.f12",
        "sequence": [
            "workbench.action.files.saveAll",
            {"command": "workbench.action.terminal.sendSequence","args": {"text":
                "\u000D clear ; make all \u000D"
            }}]
    },
],
/* / <section:multi> */
"files.watcherExclude": {
    /* \ <section:watcher> */
        "**/__pycache__/**": true,
        "**/bin/**": true, "**/include/**": true, "**/lib*/**": true,
        "**/share/**": true, "**/*.pyc": true, "**/pyvenv.cfg": true,
        "**/.pytest_cache": true,
    /* / <section:watcher> */
        "**/docs/**": true,
        "**/static/jquery.js": true, "**/static/socket.io.*": true,
},
"files.exclude": {
    /* \ <section:exclude> */
        "**/__pycache__/**": true,
        "**/bin/**": true, "**/include/**": true, "**/lib*/**": true,
        "**/share/**": true, "**/*.pyc": true, "**/pyvenv.cfg": true,
        "**/.pytest_cache": true,
    /* / <section:exclude> */
        "**/metaL/**": true,
        "**/docs/**": true,
        "**/static/jquery.js": true, "**/static/socket.io.*": true,
},
"files.associations": {
    /* \ <section:assoc> */
    "requirements.*": "config",
    /* / <section:assoc> */
},
"editor.tabSize": 4,
"workbench.tree.indent": "32",
